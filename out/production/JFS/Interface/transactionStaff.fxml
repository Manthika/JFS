<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TransactionStaff">
    <children>
        <Label layoutX="257.0" layoutY="23.0" text="TRANSACTIONS" />
        <Button fx:id="backButton" layoutX="39.0" layoutY="19.0" mnemonicParsing="false" onAction="#backButtonClicked" text="BACK" />
        <TableView fx:id="tTable" layoutX="21.0" layoutY="100.0" prefHeight="273.0" prefWidth="558.0">
            <columns>
                <TableColumn fx:id="tIdC" prefWidth="24.0" text="ID" />
                <TableColumn fx:id="tNameC" prefWidth="109.0" text="CUSTOMER NAME" />
                <TableColumn fx:id="tEmailC" prefWidth="104.0" text="EMAIL" />
                <TableColumn fx:id="tAmountC" prefWidth="69.0" text="AMOUNT" />
                <TableColumn fx:id="tBoughtC" prefWidth="151.0" text="THINGS BOUGHT" />
                <TableColumn fx:id="tDateC" prefWidth="100.0" text="DATE/TIME" />
            </columns>
        </TableView>
      <Label layoutX="31.0" layoutY="58.0" text="SEARCH BY CUSTOMER NAME" />
      <TextField fx:id="searchName" layoutX="206.0" layoutY="54.0" />
      <Button fx:id="nameSearchBtn" layoutX="364.0" layoutY="54.0" mnemonicParsing="false" onAction="#nameSearchBtnClicked" text="SEARCH" />
    </children>

</AnchorPane>
